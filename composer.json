{
    "name": "anrdaemon/net-url",
    "description": "A class to simplify handling of the various URL's (and some URI's)",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Andrey Repin",
            "email": "anrdaemon@yandex.ru"
        }
    ],
    "require": {
        "php": "^5.3.6 || ^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-trunk": "0.x-dev",
            "dev-master": "0.x-dev"
        }
    },
    "autoload": {
